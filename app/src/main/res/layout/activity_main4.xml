        android:textColor="#2C2121"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/textView21"
        app:layout_constraintTop_toTopOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="111dp"
